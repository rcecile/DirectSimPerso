PRO bdt_head

href=headfits("/sps/lsst/data/rcecile/TJP_BAO/cat_G2_Slice60.fits",ext=1)

hjs=headfits("/sps/lsst/data/rcecile/TJP_BAO/file0.fits",ext=1)

